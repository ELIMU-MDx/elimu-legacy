{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ext-zip": "*",
        "alexbowers/nova-prepopulate-searchable": "^1.0",
        "barryvdh/laravel-dompdf": "^0.8.3",
        "beyondcode/laravel-dump-server": "^1.2",
        "doctrine/dbal": "^2.9",
        "epartment/nova-dependency-container": "^1.1",
        "fideloper/proxy": "^4.0",
        "laravel-notification-channels/telegram": "^0.0.5",
        "laravel/framework": "5.8.*",
        "laravel/nova": "^2.0",
        "laravel/telescope": "1.*",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^3.1",
        "maatwebsite/laravel-nova-excel": "^1.0",
        "nao-pon/flysystem-google-drive": "~1.1",
        "nathanmac/parser": "^4.3",
        "owen-it/laravel-auditing": "^9.0",
        "predis/predis": "^1.1",
        "reinink/advanced-eloquent": "^0.2.0",
        "sparclex/nova-creatable-belongs-to": "^1.0",
        "sparclex/nova-import-card": "^2.0",
        "spatie/laravel-backup": "^6.0",
        "spatie/laravel-collection-macros": "^4.2",
        "titasgailius/search-relations": "^1.0",
        "treestoneit/belongs-to-field": "^0.1.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laracasts/generators": "^1.1",
        "lorisleiva/laravel-deployer": "^0.2.5",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
